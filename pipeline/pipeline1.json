{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "a.txt"
					},
					{
						"name": "Target",
						"value": "b.txt"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						}
					},
					"enableStaging": false,
					"translator": {
						"value": "@json(pipeline().parameters.column-mapping)",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "AzureBlobStorageSinkFileStore2",
						"type": "DatasetReference",
						"parameters": {
							"folder": "dummy-1",
							"file-name": "a.txt"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlDWTable1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"column-mapping": {
				"type": "string",
				"defaultValue": "'{\"type\":\"TabularTranslator\",\"columnMappings\":{\"Prop_0\":\"B\",\"Prop_1\":\"A\"}}'"
			},
			"myfile": {
				"type": "string",
				"defaultValue": "'a.txt'"
			}
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}